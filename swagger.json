  "components": {
    "securitySchemes": {
      "JWT-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      },
      "AdminKey": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "UserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@mail.com"
          },
          "password": {
            "type": "string",
            "example": "password123!"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string"
          }
        },
        "required": [
          "access_token"
        ]
      },
      "ErrorDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "stack": {
            "type": "string"
          }
        },
        "required": [
          "message",
          "name",
          "stack"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/LoginDto"
          },
          "error": {
            "$ref": "#/components/schemas/ErrorDto"
          }
        },
        "required": [
          "success",
          "data",
          "error"
        ]
      },
      "IsUserLoggedDto": {
        "type": "object",
        "properties": {
          "logged": {
            "type": "boolean"
          }
        },
        "required": [
          "logged"
        ]
      },
      "IsUserLoggedResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/IsUserLoggedDto"
          },
          "error": {
            "$ref": "#/components/schemas/ErrorDto"
          }
        },
        "required": [
          "success",
          "data",
          "error"
        ]
      },
      "LogResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "error": {
            "$ref": "#/components/schemas/ErrorDto"
          }
        },
        "required": [
          "success",
          "data",
          "error"
        ]
      }
    }
